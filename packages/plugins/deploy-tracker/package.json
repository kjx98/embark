{
  "name": "embark-deploy-tracker",
  "version": "6.0.0",
  "author": "Iuri Matias <iuri.matias@gmail.com>",
  "contributors": [],
  "description": "Contract deployment tracker for Embark",
  "homepage": "https://github.com/embarklabs/embark/tree/master/packages/plugins/deploy-tracker#readme",
  "bugs": "https://github.com/embarklabs/embark/issues",
  "keywords": [
    "blockchain",
    "dapps",
    "ethereum",
    "ipfs",
    "serverless",
    "solc",
    "solidity"
  ],
  "files": [
    "dist",
    "index.js"
  ],
  "license": "MIT",
  "repository": {
    "directory": "packages/plugins/deploy-tracker",
    "type": "git",
    "url": "https://github.com/embarklabs/embark.git"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "embark-collective": {
    "build:node": true,
    "typecheck": {
      "exclude": [
        "src/test"
      ]
    }
  },
  "scripts": {
    "_build": "npm run solo -- build",
    "_typecheck": "npm run solo -- typecheck",
    "ci": "npm run qa",
    "clean": "npm run reset",
    "lint": "eslint src/",
    "qa": "npm-run-all lint _typecheck _build test",
    "reset": "npx rimraf .nyc_output coverage dist embark-*.tgz package",
    "solo": "embark-solo",
    "test": "nyc --reporter=html --reporter=json mocha \"dist/test/**/*.js\" --exit --no-timeouts --require source-map-support/register"
  },
  "eslintConfig": {
    "extends": "../../../.eslintrc.json"
  },
  "dependencies": {
    "colors": "1.3.2",
    "embark-i18n": "^6.0.0",
    "embark-logger": "^6.0.0",
    "embark-utils": "^6.0.0",
    "fs-extra": "8.1.0",
    "web3": "1.2.6"
  },
  "devDependencies": {
    "embark-solo": "^6.0.0",
    "eslint": "6.8.0",
    "expect.js": "0.3.1",
    "mocha": "7.1.0",
    "npm-run-all": "4.1.5",
    "nyc": "13.1.0",
    "rimraf": "3.0.0",
    "sinon": "7.4.1"
  },
  "engines": {
    "node": ">=10.17.0",
    "npm": ">=6.11.3",
    "yarn": ">=1.19.1"
  },
  "nyc": {
    "exclude": [
      "**/node_modules/**",
      "coverage/**",
      "dist/test/**"
    ]
  }
}
